                  RowBox[{"j", "<", 
                   RowBox[{"Abs", "@", 
                    RowBox[{
                    RowBox[{"(", 
                    RowBox[{"Differences", "@", "extractedfronts1"}], ")"}], 
                    "[", 
                    RowBox[{"[", 
                    RowBox[{"i", ",", "1"}], "]"}], "]"}]}]}], ",", 
                  RowBox[{"j", "++"}], ",", 
                  RowBox[{"{", "\[IndentingNewLine]", 
