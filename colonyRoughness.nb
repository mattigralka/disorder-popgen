                    RowBox[{"[", 
                    RowBox[{"i", ",", "1"}], "]"}], "]"}], "]"}]}], ",", 
                    "0"}], "}"}]}]}], "]"}], ",", "\[IndentingNewLine]", 
                    RowBox[{
                    RowBox[{"jumpindex", "+=", "1"}], ";"}]}], 
                   "\[IndentingNewLine]", "}"}]}], "]"}], ",", 
                "\[IndentingNewLine]", 
                RowBox[{"For", "[", 
                 RowBox[{
                  RowBox[{"j", "=", "1"}], ",", 
