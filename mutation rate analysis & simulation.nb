Cell["\<\
The simulation is simple: start with a small, random number of cells. Each \
cell divides at rate kWT, mutates at rate \[Mu]. The mutants grow at rate \
kMT. Grow all cells for 7-8 generation (in accord with experiment). Repeat \
50000 times for each set of parameters (kMT, \[Mu]).\
\>", "Text",
 CellChangeTimes->{{3.7595816333605776`*^9, 3.7595817144157705`*^9}, {
  3.7595817457920017`*^9, 
  3.759581775231866*^9}},ExpressionUUID->"85ebf94b-2796-4019-a506-\
409c2f63030b"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"kWT", "=", "1"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"\[Mu]", "=", 
   RowBox[{"0.5", " ", 
    SuperscriptBox["10", 
     RowBox[{"-", "2"}]]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"kMT", "=", "1."}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"dt", "=", "1"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"repeats", "=", "50000"}], ";"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"mList", "=", 
   RowBox[{
    SuperscriptBox["10", 
     RowBox[{"-", "3"}]], 
    RowBox[{"Range", "[", 
     RowBox[{"1", ",", "16", ",", "1"}], "]"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"hist", "=", 
   RowBox[{"Table", "[", 
    RowBox[{"0", ",", 
     RowBox[{"{", 
      RowBox[{"Length", "@", "mList"}], "}"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"kList", "=", 
   RowBox[{"Range", "[", 
    RowBox[{"0.7", ",", "1.3", ",", "0.05"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"hist2", "=", 
   RowBox[{"Table", "[", 
    RowBox[{"0", ",", 
     RowBox[{"{", 
      RowBox[{"Length", "@", "kList"}], "}"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Monitor", "[", 
   RowBox[{
    RowBox[{"For", "[", 
     RowBox[{
      RowBox[{"k", "=", "1"}], ",", 
      RowBox[{"k", "\[LessEqual]", 
       RowBox[{"Length", "@", "kList"}]}], ",", 
      RowBox[{"k", "++"}], ",", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"kMT", "=", 
         RowBox[{"kList", "[", 
          RowBox[{"[", "k", "]"}], "]"}]}], ",", 
        RowBox[{"For", "[", 
         RowBox[{
          RowBox[{"m", "=", "1"}], ",", 
          RowBox[{"m", "\[LessEqual]", 
           RowBox[{"Length", "@", "mList"}]}], ",", 
          RowBox[{"m", "++"}], ",", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"\[Mu]", "=", 
             RowBox[{"mList", "[", 
              RowBox[{"[", "m", "]"}], "]"}]}], ",", 
            RowBox[{
             RowBox[{"n2", "=", 
              RowBox[{"Table", "[", 
               RowBox[{"0", ",", 
                RowBox[{"{", "repeats", "}"}]}], "]"}]}], ";", 
             RowBox[{"For", "[", 
              RowBox[{
               RowBox[{"i", "=", "1"}], ",", 
               RowBox[{"i", "\[LessEqual]", "repeats"}], ",", 
               RowBox[{"i", "++"}], ",", 
               RowBox[{"{", "\[IndentingNewLine]", 
                RowBox[{
                 RowBox[{"n0", "=", 
                  RowBox[{"RandomVariate", "[", 
                   RowBox[{"PoissonDistribution", "[", "2", "]"}], "]"}]}], 
                 ";", "\[IndentingNewLine]", 
                 RowBox[{"x", "=", "n0"}], ";", "\[IndentingNewLine]", 
