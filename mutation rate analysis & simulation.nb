                 RowBox[{"For", "[", 
                  RowBox[{
                   RowBox[{"t", "=", "1"}], ",", 
                   RowBox[{"t", "\[LessEqual]", "8"}], ",", 
                   RowBox[{"t", "+=", "dt"}], ",", 
                   RowBox[{"{", "\[IndentingNewLine]", 
                    RowBox[{
                    RowBox[{"If", "[", 
                    RowBox[{
                    RowBox[{
                    RowBox[{"t", "\[Equal]", "8"}], "&&", 
                    RowBox[{
                    RowBox[{"RandomReal", "[", "]"}], ">", "0.7"}]}], ",", 
                    "\[IndentingNewLine]", 
                    RowBox[{
