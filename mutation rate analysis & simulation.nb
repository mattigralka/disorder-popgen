                    RowBox[{"If", "[", 
                    RowBox[{
                    RowBox[{"x", ">", "0"}], ",", 
                    RowBox[{"RandomVariate", "[", 
                    RowBox[{"PoissonDistribution", "[", 
                    RowBox[{"x", " ", "kWT", " ", "dt"}], "]"}], "]"}], ",", 
                    "0"}], "]"}]}], ";", "\[IndentingNewLine]", 
                    RowBox[{"y", "+=", 
                    RowBox[{
                    RowBox[{"If", "[", 
                    RowBox[{
                    RowBox[{"y", ">", "0"}], ",", 
                    RowBox[{"RandomVariate", "[", 
                    RowBox[{"PoissonDistribution", "[", 
                    RowBox[{"y", " ", "kMT", " ", "dt"}], "]"}], "]"}], ",", 
                    "0"}], "]"}], "+", 
                    RowBox[{"If", "[", 
                    RowBox[{
                    RowBox[{"x", ">", "0"}], ",", 
                    RowBox[{"RandomVariate", "[", 
