                    RowBox[{"PoissonDistribution", "[", 
                    RowBox[{"x", " ", "kWT", " ", "dt"}], "]"}], "]"}], ",", 
                    "0"}], "]"}]}], ";", "\[IndentingNewLine]", 
                    RowBox[{"y", "+=", 
                    RowBox[{
                    RowBox[{"If", "[", 
                    RowBox[{
                    RowBox[{"y", ">", "0"}], ",", 
                    RowBox[{"RandomVariate", "[", 
                    RowBox[{"PoissonDistribution", "[", 
                    RowBox[{"y", " ", "kMT", " ", "dt"}], "]"}], "]"}], ",", 
                    "0"}], "]"}], "+", 
                    RowBox[{"If", "[", 
                    RowBox[{
                    RowBox[{"x", ">", "0"}], ",", 
                    RowBox[{"RandomVariate", "[", 
                    RowBox[{"PoissonDistribution", "[", 
                    RowBox[{"x", " ", "\[Mu]", " ", "dt"}], "]"}], "]"}], ",",
                     "0"}], "]"}]}]}]}]}], "]"}]}], "\[IndentingNewLine]", 
                    "}"}]}], "]"}], ";", "\[IndentingNewLine]", 
                 RowBox[{
                  RowBox[{"n2", "[", 
                   RowBox[{"[", "i", "]"}], "]"}], "=", 
                  RowBox[{"{", 
                   RowBox[{"x", ",", "y"}], "}"}]}], ";"}], 
                "\[IndentingNewLine]", "}"}]}], "]"}]}], ",", 
            "\[IndentingNewLine]", 
            RowBox[{
             RowBox[{"hist", "[", 
              RowBox[{"[", "m", "]"}], "]"}], "=", "n2"}]}], "}"}]}], "]"}], 
        ",", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{
          RowBox[{"hist2", "[", 
           RowBox[{"[", "k", "]"}], "]"}], "=", 
          RowBox[{"Table", "[", 
           RowBox[{
            RowBox[{
             RowBox[{
              RowBox[{"{", 
               RowBox[{
                RowBox[{"#", "[", 
                 RowBox[{"[", 
                  RowBox[{"All", ",", "2"}], "]"}], "]"}], ",", 
                RowBox[{"#", "[", 
                 RowBox[{"[", 
                  RowBox[{"All", ",", "1"}], "]"}], "]"}]}], "}"}], "&"}], 
             "@", 
             RowBox[{"DeleteCases", "[", 
              RowBox[{
               RowBox[{"hist", "[", 
                RowBox[{"[", "i", "]"}], "]"}], ",", 
               RowBox[{"{", 
                RowBox[{"0", ",", "0"}], "}"}], ",", "2"}], "]"}]}], ",", 
            RowBox[{"{", 
             RowBox[{"i", ",", "1", ",", 
              RowBox[{"Length", "@", "hist"}]}], "}"}]}], "]"}]}], ";"}]}], 
       "\[IndentingNewLine]", "}"}]}], "]"}], ",", 
    RowBox[{"{", 
     RowBox[{"k", ",", "m", ",", "i"}], "}"}]}], "]"}], 
  ";"}], "\[IndentingNewLine]"}], "Input",
